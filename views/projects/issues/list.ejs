            option.subject || 
            option.tracker ||
            option.status ||
            option.priority ||
            option.assignee ||
            option.description ||
            option.startdate ||
            option.duedate ||
            option.estimatedtime ||
            option.spenttime ||
            option.targetversion ||
            option.author ||
            option.createddate ||
            option.updateddate ||
            option.closeddate ||
            option.parenttask ||
            option.done ? '' : 'hidden' %>>
              <div>
                <a href="/projects/issues/edit/<%=projectid%>/<%= el.issueid %>" class="btn btn-success">Edit</a>
                <a href="/projects/issues/delete/<%=projectid%>/<%= el.issueid %>" class="btn btn-danger">Delete</a>
              </div>
